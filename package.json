{
  "name": "win-sqlcipher",
  "version": "0.0.4",
  "description": "Encrypted sqlite3 for Windows",
  "keywords": [
    "sqlite3",
    "sqlite",
    "sqlcipher",
    "encrypt"
  ],
  "repository": "git://github.com/ngocviet/win-sqlcipher.git",
  "scripts": {
    "nwtest": "nw nw",
    "nwbuild": "npm i --runtime=node-webkit --target=0.12.3 --target_arch=ia32",
    "electron-test": "electron electron",
    "electron-build": "electron-rebuild -w . -v 3.1.8",
    "preinstall": "node preinstall",
    "postinstall": "node postinstall"
  },
  "dependencies": {
    "shelljs": "^0.7.4",
    "sqlite3": "git+https://github.com/ngocviet/node-sqlite3.git#openssl-root"
  },
  "devDependencies": {
    "electron-rebuild": "^1.8.4"
  }
}
